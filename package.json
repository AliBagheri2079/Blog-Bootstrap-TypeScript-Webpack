{
  "name": "blog-bootstrap-typescript-webpack",
  "title": "Typescript Webpack Blog",
  "description": "A starter template/boilerplate built with TypeScript/JavaScript, Babel, Webpack 5, SASS, Bootstrap and GSAP.",
  "main": "index.js",
  "url": "URL",
  "target": "web",
  "private": false,
  "license": "MIT",
  "version": "4.0.1",
  "copyright": "2013",
  "author": "Ali Bagheri",
  "repository": "https://github.com/AliBagheri2079/Blog-Typescript-Webpack.git",
  "scripts": {
    "lint:all": "yarn lint:scripts && yarn check-types",
    "lint:all:fix": "yarn lint:scripts:fix",
    "lint:scripts": "eslint --ignore-path .gitignore **/*.{js,jsx,json,ts,tsx}",
    "lint:scripts:fix": "yarn lint:scripts --fix",
    "prepare": "husky install",
    "serve": "cross-env NODE_ENV=development webpack serve --config webpack.config.babel.js",
    "build": "webpack --progress --config webpack.config.babel.js",
    "check-types": "tsc --project tsconfig.json --noEmit",
    "check-size": "webpack --config webpack.config.babel.js --profile --json > webpack-stats.json && webpack-bundle-analyzer webpack-stats.json"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.18.2",
    "@babel/register": "^7.17.7",
    "@types/bootstrap": "^5.2.0",
    "@types/lodash": "^4.14.182",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "core-js": "3",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-webpack-plugin": "^3.1.1",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "image-minimizer-webpack-plugin": "^3.2.3",
    "imagemin": "^8.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-svgo": "^10.0.1",
    "lint-staged": "^13.0.3",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^2.6.1",
    "prettier": "^2.7.1",
    "purgecss-webpack-plugin": "^4.1.3",
    "sass": "^1.54.0",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.3",
    "ts-loader": "^9.3.0",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^v4.9.2",
    "webpack-merge": "^5.8.0"
  },
  "keywords": [
    "SASS",
    "Bootstrap",
    "Babel",
    "ES6",
    "ESLint",
    "Prettier",
    "JavaScript",
    "Template",
    "TypeScript",
    "Webpack 5",
    "GSAP"
  ],
  "browserslist": [
    "> 5%",
    "not dead",
    "not IE < 11"
  ],
  "dependencies": {
    "@popperjs/core": "^2.11.5",
    "animejs": "^3.2.1",
    "bootstrap": "^5.2.0",
    "gsap": "^3.10.4"
  }
}
